@page
@model IndexModel
@{
    ViewData["Title"] = "Animal";
}

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Sound</th>
        </tr>
    </thead>
    
    @foreach (var a in Model.animals)
    {
        <tr>
            @foreach(var i in a.Key)
            {
               <td>@i</td>   
            }
            @foreach (var i in a.Value)
            {
                <td>@i</td>  
            }
    </tr>
    }
</table>